        <name>Competitors_Must_Be_Completed</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- When Stage = Closed Won or Closed Lost, then Incumbent to be Replaced must be completed. --></errorMessage>
        <name>Incumbent_to_be_Replaced_Must_Be_Complet</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Inform Only:  When the Opportunity stage = &quot;Closed Lost&quot; then the &quot;Lost Reason Comments&quot; field must be filled out. --></errorMessage>
        <name>Lost_Reason_Comments_Must_be_completed</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- When the Opportunity stage = &quot;Closed Lost&quot; then the &quot;Lost Reason&quot; field must be filled out. --></errorMessage>
        <name>Lost_Reason_Must_be_completed</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- If Stage = Active Opportunity, then Max Probability % entered cannot be &gt; 50% or less than 5% --></errorMessage>
        <name>Opportunity_Probability_Active_Opp</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- If Stage = Evaluation, then Max Probability % entered cannot be &gt;70% or less than 5% --></errorMessage>
        <name>Opportunity_Probability_Evaluation</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- If Stage = Opportunity Identified, then Max Probability % entered cannot be &gt; 30% and cannot be less than 5% --></errorMessage>
        <name>Opportunity_Probability_OppIdent</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- If Stage = Procurement, then Max Probability % entered cannot be &gt;60% or less than 5% --></errorMessage>
        <name>Opportunity_Probability_Procurement</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- If Stage = Selected, then Max Probability % entered must be 95% --></errorMessage>
        <name>Opportunity_Probability_Selected</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- If Stage = Shortlisted, then Max Probability % entered cannot be &gt;80% or less than 5% --></errorMessage>
        <name>Opportunity_Probability_Shortlisted</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Inform Only:  When Stage = Active or beyond, then Product(s) Required must be completed. --></errorMessage>

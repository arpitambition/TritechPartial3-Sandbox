<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>These settings allow configuration of automated steps on support tickets - drives the landing page and ticket record types for any new implementations or edits to existing product families.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Contract_Type__c</fullName>
        <description>Match this field to the contract type the ticket is created from</description>
        <externalId>false</externalId>
        <label>Contract Type</label>
        <length>50</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Landing_Page__c</fullName>
        <description>Use this setting to determine the landing page for the for the record type</description>
        <externalId>false</externalId>
        <label>Landing Page</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Record_Type__c</fullName>
        <description>Match this field to the record type you desire for the contract type</description>
        <externalId>false</externalId>
        <label>Record Type</label>
        <length>50</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Support Ticket Settings</label>
    <visibility>Public</visibility>
</CustomObject>

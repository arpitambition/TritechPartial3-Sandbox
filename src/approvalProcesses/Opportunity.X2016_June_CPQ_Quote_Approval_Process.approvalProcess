            <criteriaItems>
                <field>Opportunity.Price_Book__c</field>
                <operation>equals</operation>
                <value>GSA</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>

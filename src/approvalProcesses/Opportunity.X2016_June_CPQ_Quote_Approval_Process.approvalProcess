        <entryCriteria>
            <booleanFilter>1 AND 2 AND 3 AND 4 AND (5 OR 6)</booleanFilter>
            <criteriaItems>
                <field>Opportunity.Total_Custom_Solution__c</field>
                <operation>equals</operation>
                <value>0</value>
            </criteriaItems>
            <criteriaItems>
                <field>Opportunity.Discount_Amount__c</field>
                <operation>equals</operation>
                <value>0</value>
            </criteriaItems>
            <criteriaItems>
                <field>Opportunity.Quote_Total__c</field>
                <operation>lessOrEqual</operation>
                <value>25000</value>
            </criteriaItems>
            <criteriaItems>
                <field>Opportunity.Charge_Types__c</field>
                <operation>notContain</operation>
                <value>Custom</value>
            </criteriaItems>
            <criteriaItems>
                <field>Opportunity.Price_Book__c</field>
                <operation>equals</operation>
